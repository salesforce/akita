{
  "name": "akita",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "c": "git-cz",
    "start:ng": "nx serve --project ng",
    "start:docs": "npm start --prefix=docs",
    "test": "npm run affected:test",
    "build": "nx build akita && npm run build --prefix packages/akita-schematics",
    "test:all": "nx run-many --target=test --exclude=ng-playground --all",
    "build:all": "nx run-many --target=build --all --exclude=ng-playground",
    "affected": "nx affected",
    "format": "nx format:write",
    "update": "nx migrate latest",
    "migration": "nx migrate --run-migrations",
    "postinstall": "ngcc --properties es2020 browser module main",
    "cz": "cz",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.6",
    "@angular/common": "14.2.6",
    "@angular/compiler": "14.2.6",
    "@angular/core": "14.2.6",
    "@angular/forms": "14.2.6",
    "@angular/platform-browser": "14.2.6",
    "@angular/platform-browser-dynamic": "14.2.6",
    "@angular/router": "14.2.6",
    "@ngneat/until-destroy": "^9.0.0",
    "copyfiles": "^2.4.1",
    "faker": "^5.5.3",
    "helpful-decorators": "^2.1.0",
    "rxjs": "~7.5.0",
    "schematics-utilities-fix": "^2.0.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.6",
    "@angular-eslint/eslint-plugin": "14.0.4",
    "@angular-eslint/eslint-plugin-template": "14.0.4",
    "@angular-eslint/template-parser": "14.0.4",
    "@angular/compiler-cli": "14.2.6",
    "@angular/language-service": "14.2.6",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-angular": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@jscutlery/semver": "^2.27.1",
    "@nrwl/angular": "14.8.4",
    "@nrwl/cli": "14.8.4",
    "@nrwl/eslint-plugin-nx": "14.8.4",
    "@nrwl/jest": "14.8.4",
    "@nrwl/js": "14.8.4",
    "@nrwl/linter": "14.8.4",
    "@nrwl/workspace": "14.8.4",
    "@types/jest": "28.1.8",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "git-cz": "4.7.1",
    "husky": "6.0.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-localstorage-mock": "^2.4.18",
    "jest-preset-angular": "12.2.2",
    "ng-packagr": "14.2.1",
    "nx": "14.8.4",
    "postcss": "^8.3.9",
    "postcss-import": "14.1.0",
    "postcss-preset-env": "7.5.0",
    "postcss-url": "10.1.3",
    "prettier": "2.6.2",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
